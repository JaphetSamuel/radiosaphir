$(".sub-navbar-section").on("click",".sub-navbar-search-toggle",function(n){var t=$(this).data("selector");$(t).toggleClass("show").find(".sub-navbar-search-input").focus();$(this).toggleClass("active");n.preventDefault()});$(window).scroll(function(){var n=$(window).scrollTop(),t=$(".navbar-fixed-top"),i=t.offset().top;n>i?$("body").addClass("sticky"):$("body").removeClass("sticky")});$($(".sub-navbar-right-menu li").filter(function(n,t){if(t.children[0].attributes.href.value==location.pathname&&(t.className="active",$("#subnavMainLink").removeClass("active")),window.location.pathname.indexOf("page")>=0){var i="/"+window.location.pathname.split("/")[1]+"/"+window.location.pathname.split("/")[2]+"/";t.children[0].attributes.href.value==i&&(t.className="active",$("#subnavMainLink").removeClass("active"))}}))