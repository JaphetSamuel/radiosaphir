var firtTimeShowModal=!0;$(window).scroll(function(){var n=$(window).scrollTop(),t=$(document).height()-1550;firtTimeShowModal?$("#subscripbeModal").addClass("activated"):$("#subscripbeModal").removeClass("activated");n>t?($("#subscripbeModal").removeClass("sticky"),firtTimeShowModal=!1):n>500?$("#subscripbeModal").addClass("sticky"):n<220&&$("#subscripbeModal").removeClass("sticky")});setTimeout(function(){$("#subscripbeModal").removeClass("sticky");firtTimeShowModal=!0},18e5)