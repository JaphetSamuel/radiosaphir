function SetVwoIdForPardot(n){var t,i,r;$('[name="vwo_id"]').val(n);$('[name="vwo_variation"]').val($.cookie("_vis_opt_exp_"+n+"_combi"));$('[name="previous_page_url"]').val($.cookie("prev_url"));$('[name="utm_term"]').val(localStorage.getItem("utm_term")=="null"?"not_provided":localStorage.getItem("utm_term"));$('[name="utm_source"]').val(localStorage.getItem("utm_source")=="null"?"direct":localStorage.getItem("utm_source"));$('[name="utm_medium"]').val(localStorage.getItem("utm_medium")=="null"?"direct":localStorage.getItem("utm_medium"));$('[name="utm_campaign"]').val(localStorage.getItem("utm_campaign")=="null"?"not_provided":localStorage.getItem("utm_campaign"));$('[name="utm_content"]').val(localStorage.getItem("utm_content")=="null"?"not_provided":localStorage.getItem("utm_content"));$('[name="utm_concept"]').val(localStorage.getItem("utm_concept")=="null"?"not_provided":localStorage.getItem("utm_concept"));$('[name="gclid"]').val(localStorage.getItem("gclid")=="null"?"":localStorage.getItem("gclid"));$("#landing_country").val(localStorage.getItem("CountryCode")=="null"?"":localStorage.getItem("CountryCode"));$("#landing_city").val(localStorage.getItem("City")=="null"?"":localStorage.getItem("City"));$("#landing_zip").val(localStorage.getItem("Zip")=="null"?"":localStorage.getItem("Zip"));t=localStorage.getItem("referrerUrl");$('[name="referrerUrl"]').val(t);t!="null"&&localStorage.getItem("utm_source")=="null"&&t!=null&&(i=!1,r=getSearchEngine(t,i),r!=t&&(i=!0),$('[name="utm_source"]').val(r),i&&fillUtmMediumAndSource(r,"organic"),i||fillUtmMediumAndSource(r,"referral"))}function validateEmail(n){var t=n;return/^\b[A-Z0-9._%-]+@[A-Z0-9.-]+\.[A-Z]{2,4}\b$/i.test(t)?!0:!1}function SetCalenderValues(n){n!=null?(localStorage.setItem("ContactFullName",$(n+" #ContactName").val()+" "+$(n+" #ContactLastName").val()),localStorage.setItem("Email",$(n+" #Email").val()),localStorage.setItem("CompanyName",$(n+" #CompanyName").val()),localStorage.setItem("Phone",$(n+" .se